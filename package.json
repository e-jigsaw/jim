    "babel-preset-stage-1": "^6.13.0",
    "babel-register": "^6.14.0",
    "koa": "^2.0.0",
    "koa-router": "^7.0.1",
    "koa-send": "^3.2.0",
    "koa-socket": "^4.4.0",
    "npm-run-all": "^3.1.0",
    "react": "^15.3.1",
    "react-dom": "^15.3.1",
    "react-redux": "^4.4.5",
    "redux": "^3.6.0",
    "redux-frr": "^0.4.0",
    "redux-logger": "^2.6.1",
